<div><h1>Editing Book</h1></div>
<%= form_with model:@book, local:true do |f| %>
  <% if @book.errors.any? %>
    <% @book.errors.full_messages.each do |massage|%>
      error:<%= massage %>
    <%end%>
  <%end%>
  <div>title</div>
  <%= f.text_field :title %>
  <div>body</div>
  <%= f.text_area :body %>
  <div></div>
  <%= f.submit "Update Book" %>
  <div><%= link_to "show", book_path(@book.id) %>|<%= link_to "back", books_path %></div>
<%end%>